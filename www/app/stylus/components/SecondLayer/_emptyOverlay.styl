.empty-overlay {
    position: fixed;
    width: 100%;
    height: 100%;
    //padding: 80px;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, .4);
    transform: translateY(100%);
    transition: 1s transform;
    will-change: transform;
    color: white;

    &.isOpen {
        transform: translateY(0%);
    }
}