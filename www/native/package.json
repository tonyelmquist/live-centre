{
  "name": "imrLiveCenter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "mac:clean": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache clean && npm i",
    "win:clean": "rmdir /s /q node_modules && npm cache clean --force && npm i && cd android && gradlew clean && cd..",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "android": "cd android && gradlew clean && cd.. && react-native run-android --variant=productionDebug",
    "android:prod": "react-native run-android --variant=productionRelease",
    "ios": "react-native run-ios"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "native-base": "^2.3.2",
    "prop-types": "^15.6.0",
    "react-dom": "^16.0.0",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-native-elements": "^0.17.0",
    "react-native-immersive": "^1.1.0",
    "react-native-orientation": "^3.1.0",
    "react-native-progress": "^3.4.0",
    "react-native-vector-icons": "^4.4.0",
    "react-native-video": "^2.0.0",
    "react-navigation": "^1.0.0-beta.13",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.1",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.3",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "21.2.1",
    "jest-enzyme": "^4.0.1",
    "prettier": "^1.7.4",
    "react-native-cli": "^2.0.1",
    "react-native-mock": "^0.3.1",
    "react-test-renderer": "16.0.0-beta.5",
    "redux-mock-store": "^1.3.0"
  }
}
